{
  "name": "rdf-terms",
  "version": "1.4.0",
  "description": "Convenience functions for handling RDFJS terms",
  "keywords": [
    "rdf",
    "rdfjs",
    "term",
    "quad",
    "triple"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/rdf-terms.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/rdf-terms.js/issues"
  },
  "homepage": "https://github.com/rubensworks/rdf-terms.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "dependencies": {
    "@rdfjs/data-model": "^1.1.1",
    "lodash.uniqwith": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "coveralls": "^3.0.0",
    "jest": "^23.4.1",
    "manual-git-changelog": "^1.0.0",
    "pre-commit": "^1.2.2",
    "stryker": "^0.29.4",
    "stryker-api": "^0.21.1",
    "stryker-jest-runner": "^1.2.1",
    "stryker-typescript": "^0.14.0",
    "ts-jest": "^23.10.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.3.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-mutation": "stryker run",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  }
}
