{
  "name": "rdf-terms",
  "version": "2.0.0",
  "description": "Convenience functions for handling RDFJS terms",
  "keywords": [
    "rdf",
    "rdfjs",
    "term",
    "quad",
    "triple"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/rdf-terms.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/rubensworks/"
  },
  "bugs": {
    "url": "https://github.com/rubensworks/rdf-terms.js/issues"
  },
  "homepage": "https://github.com/rubensworks/rdf-terms.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "index.d.ts",
    "index.js.map",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "dependencies": {
    "rdf-data-factory": "^2.0.0",
    "rdf-string": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.0.0",
    "coveralls": "^3.0.0",
    "jest": "^28.0.0",
    "manual-git-changelog": "^1.0.0",
    "pre-commit": "^1.2.2",
    "stryker": "^0.35.0",
    "stryker-api": "^0.24.0",
    "stryker-jest-runner": "^1.2.1",
    "stryker-typescript": "^0.18.0",
    "ts-jest": "^28.0.0",
    "tslint": "^6.0.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^5.7.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-mutation": "stryker run",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
